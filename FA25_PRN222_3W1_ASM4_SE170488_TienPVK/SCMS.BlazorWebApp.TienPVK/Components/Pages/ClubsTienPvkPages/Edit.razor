@code {
    [SupplyParameterFromQuery]
    private int ClubIdtienPvk { get; set; }

    [SupplyParameterFromForm]
    private ClubsTienPvk? ClubsTienPvk { get; set; }

    protected override async Task OnInitializedAsync()
    {
        ClubsTienPvk = await Service.ClubService.GetByIdAsync(ClubIdtienPvk);

        if (ClubsTienPvk is null)
        {
            NavigationManager.NavigateTo("notfound");
        }
    }

    // To protect from overposting attacks, enable the specific properties you want to bind to.
    // For more information, see https://learn.microsoft.com/aspnet/core/blazor/forms/#mitigate-overposting-attacks.
    private async Task UpdateClubsTienPvk()
    {
        try
        {
            await Service.ClubService.UpdateAsync(ClubsTienPvk!);
        }
        catch (DbUpdateConcurrencyException)
        {
            if (!await ClubsTienPvkExists(ClubsTienPvk!.ClubIdtienPvk))
            {
                NavigationManager.NavigateTo("notfound");
            }
            else
            {
                throw;
            }
        }

        NavigationManager.NavigateTo("/clubstienpvks");
    }

    private async Task<bool> ClubsTienPvkExists(int clubidtienpvk)
    {
        return await Service.ClubService.AnyAsync(clubidtienpvk);
    }
